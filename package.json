{
  "name": "backend",
  "version": "1.0.0",
  "main": "./build/index.js",
  "license": "MIT",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "watch": "babel --watch src --out-dir build --extensions '.ts'",
    "build": "babel src --out-dir build --extensions '.ts'",
    "tsc": "tsc",
    "start": "node ./build/index.js",
    "dev:start": "nodemon ./build/index.js",
    "lint": "yarn lint:types && yarn lint:format && yarn lint:es && yarn lint:prune",
    "lint:types": "tsc --noEmit",
    "lint:es": "eslint src --ext .js,.jsx,.ts,.tsx --cache --max-warnings 150",
    "lint:prune": "ts-prune -e",
    "lint:format": "prettier --check --ignore-unknown src"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^7.2.0",
    "prettier": "^2.8.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "nodemon": "^2.0.22",
    "ts-prune": "^0.10.3",
    "typescript": "^5.0.4"
  }
}
